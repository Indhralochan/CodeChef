

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int t=0,x,y,res;
		Scanner s=new Scanner(System.in);
		t=s.nextInt();
		while(t!=0){
		    x=s.nextInt();
		    y=s.nextInt();
		    res=Math.max(0,x-y);
		    System.out.println(res);
		    t--;
		}
		   	}
		   	
}
